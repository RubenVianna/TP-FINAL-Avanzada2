<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence              http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="unidadPersistencia1" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>modelo.Area</class>
    <class>modelo.Cliente</class>
    <class>modelo.CondicionPrioritaria</class>
    <class>modelo.EstadoTurno</class>
    <class>modelo.HistorialTramite</class>
    <class>modelo.Requisito</class>
    <class>modelo.TipoCliente</class>
    <class>modelo.Tramite</class>
    <class>modelo.Turno</class>
    <class>modelo.UsuarioWeb</class>
    <class>modelo.Empleado</class>
    <properties>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
      <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect"/>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/TpFinal"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="contra12"/>
    </properties>
  </persistence-unit>
</persistence>
